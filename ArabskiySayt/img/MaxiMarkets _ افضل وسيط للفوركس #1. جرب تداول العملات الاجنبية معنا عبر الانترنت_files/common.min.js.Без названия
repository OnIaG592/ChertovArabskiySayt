$(document).ready(function(){var a=$(".menu-button"),b=$(".mm-nav-bar__list"),c=$(".mobile-overlay");a.click(function(){$("body").toggleClass("menu-open"),b.toggleClass("mm-nav-bar--show"),c.show()}),c.click(function(){$("body").removeClass("menu-open"),b.removeClass("mm-nav-bar--show"),c.hide()}),$(".agree-terms-conditions-checkbox").attr("id","inputCheckBox"),$(".agree-terms-conditions-label").attr("for","inputCheckBox"),$(window).scroll(function(){$("body").hasClass("home")?$(this).scrollTop()>900?$("body").addClass("top-menu-sticky"):$("body").removeClass("top-menu-sticky"):$(this).scrollTop()>600?$("body").addClass("top-menu-sticky"):$("body").removeClass("top-menu-sticky")}),$(".ar-localization .mm-time__btn--today").removeClass("fadeInLeftBig").addClass("fadeInRightBig"),$(".mm-page-list > li").each(function(a){if($(this).hasClass("active-side")){var b=a;$(".collapsible").collapsible("open",b)}});var d=$(".mm-page-content table");d.length>0&&d.wrap("<div class='table-wrap'></div>");var e=$(".mm-control__link--windows"),f="https://trading.maximarkets.org/terminal/MaxiMarkets_Terminal_Windows_1.1.2_64bit.exe",g="https://trading.maximarkets.org/terminal/MaxiMarkets_Terminal_Windows_1.1.2_32bit.exe";e.length>0&&(navigator.userAgent.indexOf("WOW64")!=-1||navigator.userAgent.indexOf("Win64")!=-1?e.attr("href",f):e.attr("href",g))});